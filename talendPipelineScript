pipeline {
	agent any

	stages {
		stage('build'){

			steps{
				withMaven(maven: 'MVN353'){

				sh 'mvn clean compile'
				} 
				
			}
		}
		stage('test'){
			steps{
			withMaven(maven: 'MVN353'){
					sh 'mvn test'
					junit 'reports/**/*.xml'
				}
			}
		}
		stage('Deploy'){
			steps{
				withMaven(maven: 'MVN353'){

					sh 'make deploy'
				}			
			}
		}

	}


}