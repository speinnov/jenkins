template_standalonejob_pom_maven_script=<?xml version\="1.0" encoding\="UTF-8"?>\n\n<project xmlns\="http\://maven.apache.org/POM/4.0.0" xmlns\:xsi\="http\://www.w3.org/2001/XMLSchema-instance"\n\txsi\:schemaLocation\="http\://maven.apache.org/POM/4.0.0 http\://maven.apache.org/xsd/maven-4.0.0.xsd">\n\t<modelVersion>4.0.0</modelVersion>\n\t<parent>\n\t\t<groupId>@ProjectGroupId@</groupId>\n\t\t<artifactId>@ProjectArtifactId@</artifactId>\n\t\t<version>@ProjectVersion@</version>\n\t\t<relativePath>./pom.xml</relativePath>\n\t</parent>\n\t<groupId>@JobGroupId@</groupId>\n\t<artifactId>@JobArtifactId@</artifactId>\n\t<version>@JobVersion@</version>\n\t<name>@ProjectName@ @JobName@ (@JobType@)</name>\n\n\t<properties>\n\t\t<talend.job.path>@JobPath@</talend.job.path>\n\t\t<talend.job.package>@JobPackage@</talend.job.package>\n\n\t\t<\!-- for jobInfo.properties -->\n\t\t<talend.project.name>@ProjectName@</talend.project.name>\n\t\t<talend.project.name.lowercase>@ProjectName@</talend.project.name.lowercase>\n\t\t<talend.project.id>@ProjectId@</talend.project.id>\n\t\t<talend.project.branch>@ProjectBranch@</talend.project.branch>\n\t\t<talend.job.name>@JobName@</talend.job.name>\n\t\t<talend.job.version>@TalendJobVersion@</talend.job.version>\n\t\t<talend.job.date>@JobDate@</talend.job.date>\n\t\t<talend.job.context>@JobContext@</talend.job.context>\n\t\t<talend.job.id>@JobId@</talend.job.id>\n\t\t<talend.job.type>@JobType@</talend.job.type>\n\t\t<talend.job.framework>@Framework@</talend.job.framework>\n\t\t<talend.job.class>@JobClass@</talend.job.class>\n\t\t<talend.job.stat>@JobStat@</talend.job.stat>\n\t\t<talend.job.applyContextToChildren>@JobApplyContextToChildren@</talend.job.applyContextToChildren>\n\t\t<talend.product.version>@ProductVersion@</talend.product.version>\n\n\t\t<\!-- for bat/sh -->\n\t\t<talend.job.jvmargs>@JobJvmArgs@</talend.job.jvmargs>\n\t\t<talend.job.bat.classpath>@JobBatClasspath@</talend.job.bat.classpath>\n\t\t<talend.job.bat.addition>@JobBatAddition@</talend.job.bat.addition>\n\t\t<talend.job.sh.classpath>@JobShClasspath@</talend.job.sh.classpath>\n\t\t<talend.job.sh.addition>@JobShAddition@</talend.job.sh.addition>\n\n\t\t<talend.job.finalName>@JobFinalName@</talend.job.finalName>\n\n\t\t<\!-- for global properties -->\n\t\t<current.resources.dir>${basedir}/src/main/resources</current.resources.dir>\n\t\t<output.resources.dir>${talend.job.name}/src/main/resources</output.resources.dir>\n\t</properties>\n\n\n\t<dependencies>\n\t\t<\!-- Add some dependencies jars -->\n\t</dependencies>\n\n\t<build>\n\t\t<finalName>${talend.job.finalName}</finalName>\n\t\t<resources>\n            <resource>\n                <directory>${project.build.sourceDirectory}</directory>\n                <includes>                      \n                    <include>**/*.wsdl</include>\n                </includes>\n            </resource>\n        </resources>\n\t\t<plugins>\n\n\n\n\t\t\t<plugin>\n\t\t\t\t<groupId>org.apache.maven.plugins</groupId>\n\t\t\t\t<artifactId>maven-clean-plugin</artifactId>\n\t\t\t\t<configuration>\n\t\t\t\t\t<skip>true</skip>\n\t\t\t\t</configuration>\n\t\t\t</plugin>\n\t\t\t<plugin>\n\t\t\t\t<groupId>org.apache.maven.plugins</groupId>\n\t\t\t\t<artifactId>maven-compiler-plugin</artifactId>\n\t\t\t\t<configuration>\n\t\t\t\t\t<includes>\n\t\t\t\t\t\t<\!-- only compile the job source -->\n\t\t\t\t\t\t<include>${talend.job.path}/**.java</include>\n\t\t\t\t\t</includes>\n\t\t\t\t\t<testExcludes>\n\t\t\t\t\t\t<\!-- exclude every test -->\n\t\t\t\t\t\t<exclude>**</exclude>\n\t\t\t\t\t</testExcludes>\n\t\t\t\t</configuration>\n\t\t\t</plugin>\n\t\t\t<plugin>\n\t\t\t\t<groupId>org.apache.maven.plugins</groupId>\n\t\t\t\t<artifactId>maven-surefire-plugin</artifactId>\n\t\t\t\t<configuration>\n\t\t\t\t\t<skip>true</skip>\n\t\t\t\t</configuration>\n\t\t\t</plugin>\n\t\t\t<plugin>\n\t\t\t\t<groupId>org.apache.maven.plugins</groupId>\n\t\t\t\t<artifactId>maven-resources-plugin</artifactId>\n\t\t\t\t<executions>\n\t\t\t\t\t<execution>\n\t\t\t\t\t\t<id>copy-running-resources</id>\n\t\t\t\t\t\t<phase>compile</phase>\n\t\t\t\t\t\t<goals>\n\t\t\t\t\t\t\t<goal>copy-resources</goal>\n\t\t\t\t\t\t</goals>\n\t\t\t\t\t\t<configuration> <\!-- copy the contexts for running job -->\n\t\t\t\t\t\t\t<outputDirectory>${project.build.outputDirectory}/${talend.job.path}</outputDirectory>\n\t\t\t\t\t\t\t<resources>\n\t\t\t\t\t\t\t\t<resource>\n\t\t\t\t\t\t\t\t\t<directory>${current.resources.dir}/${talend.job.path}</directory>\n\t\t\t\t\t\t\t\t\t<includes>\n\t\t\t\t\t\t\t\t\t\t<include>contexts/*.properties</include>\n\t\t\t\t\t\t\t\t\t</includes>\n\t\t\t\t\t\t\t\t</resource>\n\t\t\t\t\t\t\t</resources>\n\t\t\t\t\t\t</configuration>\n\t\t\t\t\t</execution>\n\t\t\t\t</executions>\n\t\t\t</plugin>\n            <plugin>\n              <groupId>org.apache.maven.plugins</groupId>\n              <artifactId>maven-jar-plugin</artifactId>\n              <executions>\n                <execution>\n                  <id>default-jar</id>\n                  <phase>package</phase>\n                  <goals>\n                    <goal>jar</goal>\n                  </goals>\n                  <configuration>\n                    <includes>\n                      <\!-- only package the job classes -->\n                      <include>${talend.job.path}/*.class</include>\n                      <include>${talend.job.path}/**/*.wsdl</include>\n                    </includes>\n                  </configuration>\n                </execution>\n             </executions>\n           </plugin>\t\t\t\n\t\t </plugins>\n\t</build>\n\t<profiles>\n\t    <profile>\n\t       <id>packaging-and-assembly</id>\n            <activation>\n                <activeByDefault>true</activeByDefault>\n            </activation>\t       \n\t       <build>\n\t         <plugins>\n               <plugin>\n                 <groupId>org.apache.maven.plugins</groupId>\n                 <artifactId>maven-assembly-plugin</artifactId>\n                 <executions>\n                   <execution>\n                     <phase>package</phase>\n                     <goals>\n                       <goal>single</goal>\n                     </goals>\n                     <configuration>\n                       <descriptors>\n                         <descriptor>${basedir}/src/main/assemblies/assembly_${talend.job.name}_${talend.job.version}.xml</descriptor>\n                       </descriptors>\n                       <tarLongFileMode>gnu</tarLongFileMode>\n                       <appendAssemblyId>false</appendAssemblyId>\n                       <finalName>${talend.job.finalName}</finalName>\n                     </configuration>\n                   </execution>\n                </executions>\n              </plugin>\n            </plugins>  \n\t      </build>\n\t    </profile>\n\t\t<profile>\n\t\t\t<id>default-settings</id>\n\t\t\t<activation>\n\t\t\t\t<activeByDefault>true</activeByDefault>\n\t\t\t</activation>\n\t\t\t<properties>\n\t\t\t\t<\!-- by default, all excluded, fixing that when "include-libs" profile \n\t\t\t\t\tis not active, export the libraries always. -->\n\t\t\t\t<talend.libraries.excludes.set>*\:*</talend.libraries.excludes.set>\n\t\t\t\t<talend.jobs.excludes.set>*\:*</talend.jobs.excludes.set>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-java-sources</id>\n\t\t\t<activation>\n\t\t\t\t<activeByDefault>true</activeByDefault>\n\t\t\t</activation>\n\t\t\t<properties>\n\t\t\t\t<sourcecodes.dir>${project.build.sourceDirectory}</sourcecodes.dir>\n\t\t\t\t<resources.dir>${current.resources.dir}/</resources.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-maven-resources</id>\n\t\t\t<activation>\n\t\t\t\t<activeByDefault>true</activeByDefault>\n\t\t\t</activation>\n\t\t\t<properties>\n\t\t\t\t<templates.dir>${basedir}/src/main/templates/</templates.dir>\n\t\t\t\t<assemblies.dir>${basedir}/src/main/assemblies/</assemblies.dir>\n\t\t\t\t<poms.dir>${basedir}</poms.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-items</id>\n\t\t\t<activation>\n\t\t\t\t<activeByDefault>true</activeByDefault>\n\t\t\t</activation>\n\t\t\t<properties>\n\t\t\t\t<items.dir>${basedir}/src/main/items/${project.build.finalName}</items.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-libs</id>\n\t\t\t<activation>\n\t\t\t\t<activeByDefault>true</activeByDefault>\n\t\t\t</activation>\n\t\t\t<properties>\n\t\t\t\t<talend.libraries.includes.set>org.talend.libraries\:*</talend.libraries.includes.set>\n\t\t\t\t<talend.libraries.excludes.set>${talend.libraries.includes.set}</talend.libraries.excludes.set>\n\t\t\t\t<talend.jobs.excludes.set>${project.groupId}\:*</talend.jobs.excludes.set>\n\t\t\t\t<current.job.binaries.include>${project.groupId}\:${project.artifactId}\:${project.version}</current.job.binaries.include>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-binaries</id>\n\t\t\t<activation>\n\t\t\t\t<activeByDefault>true</activeByDefault>\n\t\t\t</activation>\n\t\t\t<properties>\n\t\t\t\t<jobs.binaries.includes.set>${project.groupId}\:*</jobs.binaries.includes.set>\n\t\t\t\t<routines.binaries.includes.set>org.talend.code.${talend.project.name.lowercase}\:*</routines.binaries.includes.set>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-xmlMappings</id>\n            <activation>\n                <activeByDefault>true</activeByDefault>\n            </activation>\n\t\t\t<properties>\n\t\t\t\t<xmlMappings.resources.dir>${current.resources.dir}/xmlMappings/</xmlMappings.resources.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-running-xmlMappings</id>\n            <activation>\n                <activeByDefault>true</activeByDefault>\n            </activation>\n\t\t\t<properties>\n\t\t\t\t<xmlMappings.running.dir>\n\t\t\t\t\t${current.resources.dir}/xmlMappings/\n\t\t\t\t</xmlMappings.running.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-rules</id>\n\t\t\t<properties>\n\t\t\t\t<rules.resources.dir>${current.resources.dir}/rules/</rules.resources.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-log4j</id>\n            <activation>\n                <activeByDefault>true</activeByDefault>\n            </activation>\n\t\t\t<properties>\n\t\t\t\t<log4j.resources.dir>${current.resources.dir}/</log4j.resources.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-running-log4j</id>\n\t\t\t<properties>\n\t\t\t\t<log4j.running.dir>${current.resources.dir}/</log4j.running.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-contexts</id>\n\t\t\t<activation>\n\t\t\t\t<activeByDefault>true</activeByDefault>\n\t\t\t</activation>\n\t\t\t<properties>\n\t\t\t\t<contexts.running.dir>${current.resources.dir}/</contexts.running.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-test-sources</id>\n\t\t\t<properties>\n\t\t\t\t<test.sourcecodes.dir>${project.build.testSourceDirectory}</test.sourcecodes.dir>\n\t\t\t\t<test.resources.dir>${basedir}/src/test/resources/</test.resources.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-test-reports</id>\n\t\t\t<properties>\n\t\t\t\t<test.reports.dir>${project.build.directory}/surefire-reports</test.reports.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-pigudfs-java-sources</id>\n\t\t\t<properties>\n\t\t\t\t<pigudfs.sourcecodes.dir>${project.build.sourceDirectory}</pigudfs.sourcecodes.dir>\n\t\t\t\t<pigudfs.poms.dir>${basedir}</pigudfs.poms.dir>\n\t\t\t</properties>\n\t\t</profile>\n\t\t<profile>\n\t\t\t<id>include-pigudfs-binaries</id>\n\t\t\t<properties>\n\t\t\t\t<pigudfs.binaries.includes.set>org.talend.pigudf.${talend.project.name.lowercase}\:*</pigudfs.binaries.includes.set>\n\t\t\t</properties>\n\t\t</profile>\n\t\t\n\t\t<profile>\n            <id>include-microservice-configs</id>\n            <properties>\n                <microservice.configs.dir>${current.resources.dir}/</microservice.configs.dir>\n            </properties>\n        </profile>\n        \n        <profile>\n            <id>include-microservice-running-configs</id>\n            <properties>\n                <microservice.running.configs.dir>${current.resources.dir}/</microservice.running.configs.dir>\n            </properties>\n        </profile>\n\t    <profile>\n\t\t\t<id>include-survivorship-rules</id>\n\t\t\t<activation>\n\t\t\t\t<activeByDefault>true</activeByDefault>\n\t\t\t</activation>\n\t\t\t<properties>\n\t\t\t\t<survivorship.rules.dir>${current.resources.dir}/metadata/survivorship/</survivorship.rules.dir>\n\t\t\t</properties>\n\t    </profile>\n\n\t</profiles>\n</project>\n
